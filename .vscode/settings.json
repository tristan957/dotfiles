{
	"[lua]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 2,
		"editor.formatOnSave": true,
		"editor.defaultFormatter": "JohnnyMorganz.stylua"
	},
	"[markdown]": {
		"editor.formatOnSave": true,
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[shellscript]": {
		"files.eol": "\n",
		"editor.formatOnSave": true
	},
	"[yaml]": {
		"editor.formatOnSave": true,
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"Lua.diagnostics.globals": ["vim"],
	"Lua.runtime.version": "LuaJIT",
	"Lua.workspace.library": [
		"/usr/lib64/nvim",
		"/usr/share/nvim/runtime",
		"/usr/share/nvim/runtime/pack/dist/opt/cfilter",
		"/usr/share/nvim/runtime/pack/dist/opt/matchit",
		"~/.local/share/comlink/lua",
		"~/.local/share/nvim/lazy/lazy.nvim",
		"~/.local/share/nvim/lazy/refactoring.nvim",
		"~/.local/share/nvim/lazy/mini.diff",
		"~/.local/share/nvim/lazy/todo-comments.nvim",
		"~/.local/share/nvim/lazy/mini.hipatterns",
		"~/.local/share/nvim/lazy/nvim-treesitter-textobjects",
		"~/.local/share/nvim/lazy/nvim-treesitter-context",
		"~/.local/share/nvim/lazy/nvim-treesitter",
		"~/.local/share/nvim/lazy/mini.icons",
		"~/.local/share/nvim/lazy/cmp-dap",
		"~/.local/share/nvim/lazy/cmp-git",
		"~/.local/share/nvim/lazy/cmp-path",
		"~/.local/share/nvim/lazy/cmp-nvim-lsp-signature-help",
		"~/.local/share/nvim/lazy/cmp-nvim-lsp-document-symbol",
		"~/.local/share/nvim/lazy/cmp-emoji",
		"~/.local/share/nvim/lazy/cmp-cmdline",
		"~/.local/share/nvim/lazy/cmp-calc",
		"~/.local/share/nvim/lazy/cmp-buffer",
		"~/.local/share/nvim/lazy/nvim-cmp",
		"~/.local/share/nvim/lazy/mini.align",
		"~/.local/share/nvim/lazy/mini.bracketed",
		"~/.local/share/nvim/lazy/mini.surround",
		"~/.local/share/nvim/lazy/mini.ai",
		"~/.local/share/nvim/lazy/telescope-fzf-native.nvim",
		"~/.local/share/nvim/lazy/plenary.nvim",
		"~/.local/share/nvim/lazy/trouble.nvim",
		"~/.local/share/nvim/lazy/telescope.nvim",
		"~/.local/share/nvim/lazy/ts-comments.nvim",
		"~/.local/share/nvim/lazy/schemastore.nvim",
		"~/.local/share/nvim/lazy/mason-lspconfig.nvim",
		"~/.local/share/nvim/lazy/nvim-lspconfig",
		"~/.local/share/nvim/lazy/vim-sleuth",
		"~/.local/share/nvim/lazy/vim-fugitive",
		"~/.local/share/nvim/lazy/nvim-nio",
		"~/.local/share/nvim/lazy/nvim-dap",
		"~/.local/share/nvim/lazy/nvim-dap-ui",
		"~/.local/share/nvim/lazy/mini.move",
		"~/.local/share/nvim/lazy/fidget.nvim",
		"~/.local/share/nvim/lazy/nvim-notify",
		"~/.local/share/nvim/lazy/dressing.nvim",
		"~/.local/share/nvim/lazy/lualine.nvim",
		"~/.local/share/nvim/lazy/mason.nvim",
		"~/.local/share/nvim/lazy/mini.files",
		"~/.local/share/nvim/lazy/mini.trailspace",
		"~/.local/share/nvim/lazy/cmp-nvim-lsp",
		"~/.local/share/nvim/lazy/mini.misc",
		"~/.local/share/nvim/lazy/onedarkpro.nvim",
		"~/.local/share/nvim/lazy/cmp-dap/after",
		"~/.local/share/nvim/lazy/cmp-path/after",
		"~/.local/share/nvim/lazy/cmp-nvim-lsp-signature-help/after",
		"~/.local/share/nvim/lazy/cmp-nvim-lsp-document-symbol/after",
		"~/.local/share/nvim/lazy/cmp-emoji/after",
		"~/.local/share/nvim/lazy/cmp-cmdline/after",
		"~/.local/share/nvim/lazy/cmp-calc/after",
		"~/.local/share/nvim/lazy/cmp-buffer/after",
		"~/.local/share/nvim/lazy/cmp-nvim-lsp/after",
		"~/.local/share/nvim/lazy/onedarkpro.nvim/after",
		"~/.var/nvim/lazy/readme"
	],
	"shellcheck.useWorkspaceRootAsCwd": true,
	"shellcheck.customArgs": [
		"--shell",
		"bash",
		"--source-path",
		"bash",
		"-x"
	],
	"files.associations": {
		".bash*": "shellscript",
		"forward_as_body": "gotmpl",
		"new_message": "gotmpl",
		"quoted_reply": "gotmpl",
		"work.config": "properties",
		"ignore": "ignore"
	},
	"shellformat.flag": "-indent 4"
}

{
  "private": true,
  "scripts": {
    "markdownlint:check": "markdownlint-cli2 '**/*.md' !cargo/.opt/cargo !node_modules",
    "prettier:check": "prettier --config .prettierrc.yml --check '**/*.{yaml,yml}'",
    "prettier:format": "prettier --config .prettierrc.yml --write '**/*.{yaml,yml}'"
  },
  "devDependencies": {
    "markdownlint-cli2": "^0.21.0",
    "prettier": "^3.8.1"
  }
}
